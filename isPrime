
def main():
	n = input ("Enter a number")
	if isPrime(n) == False:
		print("Not a Prime Number")
	else:
		print("Prime number")
	
	
def isPrime(number):
	i=1
	for i in range(2,number/2):
		if number in {1,2}:
			return False
		else:
			if(number%i==0): 
				return False
			
if __name__ == "__main__": main()
		
			 
	
	